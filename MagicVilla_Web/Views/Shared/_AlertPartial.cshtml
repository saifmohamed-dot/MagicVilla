<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
@if(TempData["Success"] != null)
{
    <script>
        swal({
          title: "Done!",
          text: "@TempData["Success"]!.ToString()!",
          icon: "success",
        });
    </script>
}
@if(TempData["Error"] != null)
{
    <script>
        swal({
          title: "Sorry",
          text: "@TempData["Error"]!.ToString()!",
          icon: "error",
        });
    </script>
}